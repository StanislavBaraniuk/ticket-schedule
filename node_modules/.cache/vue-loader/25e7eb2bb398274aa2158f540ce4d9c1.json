ve: function (user) {\n            let add_user = async (user) => {\n                await window.api.client.update(window.api.storage.getToken(), user)\n            };\n\n            if (user.EMAIL.length > 0 && /.[a-zA-Z0-9]{1,}@[a-z]{1,}[.][a-z]{2,}/.test(user.EMAIL) && parseInt(user.ID) >= 0 && (/.([0-9]{11})/.test(user.PHONE) || user.PHONE.length === 0)) {\n                add_user(user).then(function () {\n                    window.location.reload();\n                });\n            } else {\n                this.error = \"Впевніться що: емайл введено, ІД >= 0, Телефон: 13 цифер\"\n            }\n        }\n    }\n}\n",{"version":3,"sources":["UserRedaction.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"UserRedaction.vue","sourceRoot":"src/components/admin/tools/Users/components","sourcesContent":["<template>\n    <v-card class=\"user-redaction\" v-if=\"activeRedactId.v === item.ID\">\n\n        <v-layout row wrap>\n            <v-flex xs12 sm12 md12 lg12>\n                <v-layout row wrap>\n                    <v-flex xs12 sm12 md6 lg6>\n                        <v-select\n                                v-model=\"item.SEX\"\n                                :items=\"sex_id\"\n                                :label=\"'Стать - ' + sex_item[item.SEX]\"\n                        ></v-select>\n                    </v-flex>\n\n                    <v-flex xs12 sm12 md6 lg6>\n                        <v-autocomplete\n                                :items=\"access_list\"\n                                label=\"Рівень доступу\"\n                                :readonly=\"false\"\n                                v-model=\"item.ACCESS\"\n                        ></v-autocomplete>\n                    </v-flex>\n\n                    <v-flex xs12 sm12 md6 lg6>\n                        <v-text-field\n                                :rules=\"emailRules\"\n                                label=\"E-mail\"\n                                v-model=\"item.EMAIL\"\n                        ></v-text-field>\n                    </v-flex>\n\n                    <v-flex xs12 sm12 md6 lg6>\n                        <v-text-field\n                                :rules=\"phoneRules\"\n                                label=\"Телефон\"\n                                v-model=\"item.PHONE\"\n                        ></v-text-field>\n                    </v-flex>\n\n                    <v-flex xs12 sm12 md6 lg6>\n                        <v-text-field\n                                label=\"Імя\"\n                                v-model=\"item.FIRST_NAME\"\n                        ></v-text-field>\n                    </v-flex>\n\n                    <v-flex xs12 sm12 md6 lg6>\n                        <v-text-field\n                                label=\"Фамілія\"\n                                v-model=\"item.LAST_NAME\"\n                        ></v-text-field>\n                    </v-flex>\n                </v-layout>\n            </v-flex>\n        </v-layout>\n\n        <v-flex xs12>\n            <v-btn dark color=\"indigo\" v-on:click=\"f.v(-1)\"><v-icon>close</v-icon></v-btn>\n            <v-btn dark color=\"indigo\" v-on:click=\"save(item)\">SAVE</v-btn>\n            <span><small style=\"color: red\">{{error}}</small></span>\n        </v-flex>\n\n    </v-card>\n</template>\n\n<script>\n    export default {\n        name: \"UserRedaction\",\n        data: function () {\n            return {\n                error: '',\n                access_list: [1,2],\n                requireRule: [\n                    v => !!v || 'Обовязкове поле'\n                ],\n                phoneRules: [\n                    v => /.([0-9]{11})/.test(v) || 'Телефон: 13 цифер'\n                ],\n                emailRules: [\n                    v => !!v || 'E-mail є обовязковим',\n                    v => /.[a-zA-Z0-9]{1,}@[a-z]{1,}[.][a-z]{2,}/.test(v) || 'E-mail повинен бути вірним'\n                ],\n                sex_id: [0,1,2],\n                sex_item: ['Не визначено','Чоловік','Жінка']\n            }\n        },\n        props: {\n            item:{\n\n            },\n            activeRedactId: {\n\n            },\n            f: {}\n        },\n        methods: {\n            save: function (user) {\n                let add_user = async (user) => {\n                    await window.api.client.update(window.api.storage.getToken(), user)\n                };\n\n                if (user.EMAIL.length > 0 && /.[a-zA-Z0-9]{1,}@[a-z]{1,}[.][a-z]{2,}/.test(user.EMAIL) && parseInt(user.ID) >= 0 && (/.([0-9]{11})/.test(user.PHONE) || user.PHONE.length === 0)) {\n                    add_user(user).then(function () {\n                        window.location.reload();\n                    });\n                } else {\n                    this.error = \"Впевніться що: емайл введено, ІД >= 0, Телефон: 13 цифер\"\n                }\n            }\n        }\n    }\n</script>\n\n<style lang=\"sass\" scoped>\n    .user-redaction\n        padding: 30px\n</style>"]}]}